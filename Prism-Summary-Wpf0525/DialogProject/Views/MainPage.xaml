<Window x:Class="DialogProject.Views.MainPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="MainPage" Height="450" Width="800">
    <StackPanel>
        <TextBlock Text="{Binding Title}" FontSize="25"/>
        <Button Command="{Binding OpenDialogCmd}" Margin="50" Width="140" Height="60" Content="{Binding BtnText}"/>
        <Button Command="{Binding PubSubCmd}" Margin="50" Width="140" Height="60" Content="发布消息"/>
        
    </StackPanel>
</Window>
